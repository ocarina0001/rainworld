<Defs>

    <ThingDef ParentName="AnimalThingBase">
        <defName>OCARINA_GreenLizard</defName>
        <label>green lizard</label>
        <description>Green lizards are a subspecies of the greater lizard tree. In comparison to other lizards, they're larger and slower which lends to their overall high durability. Green lizards are typically very aggressive, yet prove to be easy to domesticate, rarely fighting against a taming attempt.</description>
        <statBases>
            <MoveSpeed>2.75</MoveSpeed>
            <MarketValue>850</MarketValue>
            <ComfyTemperatureMin>-10</ComfyTemperatureMin>
            <ComfyTemperatureMax>50</ComfyTemperatureMax>
            <FilthRate>6</FilthRate>
        </statBases>
        <tools>
            <li>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>22</power>
                <cooldownTime>2.6</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                <chanceFactor>0.7</chanceFactor>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>14</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
        </tools>
        <comps>
            <li Class="CompProperties_EggLayer">
                <eggFertilizedDef>OCARINA_EggGreenLizardFertilized</eggFertilizedDef>
                <eggFertilizationCountMax>1</eggFertilizationCountMax>
                <eggLayIntervalDays>6</eggLayIntervalDays>
                <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
                <eggCountRange>1</eggCountRange>
            </li>
        </comps>
        <race>
            <body>OCARINA_LargeLizardlike</body>
            <herdAnimal>false</herdAnimal>
            <predator>true</predator>
            <maxPreyBodySize>2.0</maxPreyBodySize>
            <baseBodySize>1.85</baseBodySize>
            <baseHealthScale>2.5</baseHealthScale>
            <baseHungerRate>0.25</baseHungerRate>
            <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
            <leatherDef>Leather_Lizard</leatherDef>
            <wildness>0.25</wildness>
            <trainability>Advanced</trainability>
            <lifeExpectancy>80</lifeExpectancy>
            <manhunterOnDamageChance>0.2</manhunterOnDamageChance>
            <manhunterOnTameFailChance>0.01</manhunterOnTameFailChance>
            <wildBiomes>
                <TemperateSwamp>0.12</TemperateSwamp>
                <TropicalSwamp>0.24</TropicalSwamp>
                <TropicalRainforest>0.40</TropicalRainforest>
            </wildBiomes>
            <lifeStageAges>
                <li>
                    <def>AnimalBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>AnimalJuvenile</def>
                    <minAge>0.15</minAge>
                </li>
                <li>
                    <def>AnimalAdult</def>
                    <minAge>0.3</minAge>
                    <soundWounded>OCARINA_Pawn_GreenLizard_Call</soundWounded>
                    <soundDeath>OCARINA_Pawn_GreenLizard_Call</soundDeath>
                    <soundCall>OCARINA_Pawn_GreenLizard_Call</soundCall>
                    <soundAngry>OCARINA_Pawn_GreenLizard_Call</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
        <tradeTags>
            <li>AnimalUncommon</li>
            <li>AnimalFighter</li>
        </tradeTags>
    </ThingDef>
